            <TableColumn fx:id="recruiter_email" prefWidth="179.0" text="Email" />
        </columns>
      </TableView>
      <Button fx:id="btn_add" layoutX="29.0" layoutY="415.0" mnemonicParsing="false" onMouseClicked="#add_recruiter" text="Ajouter" />
      <Button fx:id="btn_edit" layoutX="97.0" layoutY="415.0" mnemonicParsing="false" onMouseClicked="#edit_recruiter" text="Modifier" />
      <Button fx:id="btn_delete" layoutX="169.0" layoutY="415.0" mnemonicParsing="false" onMouseClicked="#delete_recruiter" text="Supprimer" />
      <Button layoutX="285.0" layoutY="33.0" mnemonicParsing="false" onMouseClicked="#search_recruiter" text="search" />
   </children>
</AnchorPane>
