          <TableColumn fx:id="recruiter_name" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="recruiter_address" prefWidth="75.0" text="Address" />
            <TableColumn fx:id="recruiter_description" prefWidth="75.0" text="Description" />
