<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="recrutini.GUI.RecruiterController">
   <children>
      <Label layoutX="37.0" layoutY="37.0" text="Rechercher" />
      <TextField fx:id="txt_searchID" layoutX="121.0" layoutY="33.0" promptText="name" />
      <TextField fx:id="txt_Name" layoutX="23.0" layoutY="97.0" promptText="Name" />
      <TextField fx:id="txt_address" layoutX="23.0" layoutY="134.0" promptText="Address" />
      <TextField fx:id="txt_description" layoutX="23.0" layoutY="165.0" promptText="Description" />
      <TextField fx:id="txt_phone_number" layoutX="23.0" layoutY="200.0" promptText="Phone Number" />
      <TextField fx:id="txt_image" layoutX="23.0" layoutY="234.0" promptText="image" />
      <TextField fx:id="txt_field_id" layoutX="23.0" layoutY="271.0" promptText="Field" />
      <TextField fx:id="txt_email" layoutX="23.0" layoutY="308.0" promptText="Email" />
      <PasswordField fx:id="txt_password" layoutX="23.0" layoutY="346.0" promptText="Password" />
      <TableView fx:id="table_recruiter" layoutX="221.0" layoutY="97.0" onMouseClicked="#tablerecruiter_event" prefHeight="274.0" prefWidth="555.0">
