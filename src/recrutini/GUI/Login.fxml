<AnchorPane id="AnchorPane" prefHeight="435.0" prefWidth="588.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="recrutini.GUI.LoginController">
   <children>
      <Pane layoutX="-142.0" prefHeight="435.0" prefWidth="736.0">
         <children>
            <ImageView fx:id="imageview" fitHeight="102.0" fitWidth="304.0" layoutX="153.0" layoutY="37.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/cm-logo.png" />
               </image></ImageView>
            <TextField fx:id="mail" layoutX="445.0" layoutY="129.0" prefHeight="35.0" prefWidth="237.0" promptText="Enter your Email">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TextField>
            <PasswordField fx:id="pwd" layoutX="445.0" layoutY="183.0" prefHeight="35.0" prefWidth="237.0" promptText="Enter your password">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </PasswordField>
            <Label fx:id="btnfo" layoutX="512.0" layoutY="278.0" onMousePressed="#forgot" prefHeight="17.0" prefWidth="102.0" text="Forgot password ?" underline="true">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Label>
            <Label layoutX="490.0" layoutY="27.0" opacity="0.86" prefHeight="102.0" prefWidth="214.0" text="Sign In" textFill="#2b00ff">
               <font>
                  <Font size="55.0" />
               </font>
            </Label>
            <Label fx:id="btnfo1" layoutX="475.0" layoutY="310.0" onMousePressed="#Singup" prefHeight="17.0" prefWidth="178.0" styleClass="buttonCRUD" stylesheets="@stylesheet.css" text="Don't have an account ? Sign up" textFill="#f8f8f8" underline="true">
