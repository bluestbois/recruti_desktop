                  <Cursor fx:constant="HAND" />
               </cursor>
            </Label>
            <Button fx:id="btnin" layoutX="536.0" layoutY="235.0" mnemonicParsing="false" onAction="#signin" styleClass="button4" stylesheets="@stylesheet.css" text="Sign in " textFill="WHITE" />
            <ImageView fitHeight="209.0" fitWidth="251.0" layoutX="166.0" layoutY="201.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/cm-main-img.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="73.0" fitWidth="102.0" layoutX="527.0" layoutY="341.0" onMouseClicked="#welcome" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/left.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
   </children>
